int i=4 , j=5 , k=6 , l=7;

void setup() {
  pinMode (i,OUTPUT);
  pinMode (j,OUTPUT);
  pinMode (k,OUTPUT);
  pinMode (l,OUTPUT);
}

void loop() {
 digitalWrite(i,HIGH);
 delay(1000);
 digitalWrite(i,LOW);
 delay(1000);
 digitalWrite(j,HIGH);
 delay(1000);
 digitalWrite(j,LOW);
 delay(1000);
 digitalWrite(k,HIGH);
 delay(1000);
 digitalWrite(k,LOW);
 delay(1000);
 digitalWrite(l,HIGH);
 delay(1000);
 digitalWrite(l,LOW);
 delay(1000);
 digitalWrite(k,HIGH);
 delay(1000);
 digitalWrite(k,LOW);
 delay(1000);
 digitalWrite(j,HIGH);
 delay(1000);
 digitalWrite(j,LOW);
 delay(1000);
}
